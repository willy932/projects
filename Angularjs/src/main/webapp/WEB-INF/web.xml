<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>HTTPANGULAR</display-name>

  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>javaAngularJs</servlet-name>
    <servlet-class>com.formation.servlet.AngularJsServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ListClient</servlet-name>
    <servlet-class>com.formation.servlet.ListClientServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>javaAngularJs</servlet-name>
    <url-pattern>/javaAngularJs</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ListClient</servlet-name>
    <url-pattern>/ListClient</url-pattern>
  </servlet-mapping>

</web-app>
